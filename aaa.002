									oncomplete="initResult();" update=":form:panelresult"
									process="@this">
									<p:confirm header="Confirmaci√≥n" message="Est√° seguro?" icon="ui-icon-alert" />
								</p:commandButton>
															
								<p:commandButton styleClass="btn  btn-flat "
									title="Indexa la base de datos OFAC, necesario para realizar busquedas y SCANEOS a mensajes swift"
									value="Indexar Lista OFAC" actionListener="#{loadListOfacController.indexListaOfac}"
									process="@this">
									<p:confirm header="Confirmaci√≥n" message="Est√° seguro?" icon="ui-icon-alert" />
								</p:commandButton>
								
								<p:commandButton styleClass="btn  btn-flat "
									title="Indexa los mensajes registrados en el sistema, tarea que se realiza por razones de mantenimiento"
									value="Indexar BICs y Mensajes Swift" actionListener="#{loadListOfacController.indexListaLvd}"
									process="@this">
									<p:confirm header="Confirmaci√≥n" message="Est√° seguro de indexar? el proceso puede durar seg√∫n el tama√±o de la base de datos" icon="ui-icon-alert" />
								</p:commandButton>								
							</div>
						</div>
					</div>
				</div>
			</p:outputPanel>
			<p:remoteCommand name="ejec" actionListener="#{loadListOfacController.truncarListaOfac}" />
			<p:outputPanel id="panelresult">
				<div class="table-responsive">
					<h:dataTable id="dtableResult" var="item" value="#{loadListOfacController.swfLoteLista}" styleClass="table table-striped table-condensed">
						<h:column>
							<f:facet name="header">
								<h:outputText value="Nro. Lote" />
							</f:facet>
							<h:outputText value="#{item.lotNrolote}" />
						</h:column>
						<h:column>
							<f:facet name="header">
								<h:outputText value="Fecha"/>
							</f:facet>
							<h:outputText value="#{item.lotFechora}" />
						</h:column>
						<h:column>
							<f:facet name="header">
								<h:outputText value="Archivo" title="Nombre de archivo de lista" />
							</f:facet>
							<h:outputText value="#{item.lotNomorig}" />
						</h:column>
						<h:column>
							<f:facet name="header">
								<h:outputText value="Hash" />
							</f:facet>
							<h:outputText value="#{item.lotHash}" />
						</h:column>						
						<h:column>
							<f:facet name="header">
								<h:outputText value="Usuario" />
							</f:facet>
							<h:outputText value="#{item.lotAuditusr}" />
						</h:column>
					</h:dataTable>
				</div>
			</p:outputPanel>

			<p:confirmDialog global="true" showEffect="fade" hideEffect="explode">
				<p:commandButton value="Aceptar" type="button" styleClass="ui-confirmdialog-yes" icon="ui-icon-check" />
				<p:commandButton value="Cancelar" type="button" styleClass="ui-confirmdialog-no" icon="ui-icon-close" />
			</p:confirmDialog>

			<input type="hidden" name="${_csrf.parameterName}" value="${_csrf.token}" />
		</h:form>
            <script language="javascript">
            	initResult();
            </script>		
	</ui:define>
</ui:composition>
