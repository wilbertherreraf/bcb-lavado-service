			recuperardatos() ;
			addMessageInfo("Operaci√≥n exitosa", "La operaci√≥n fue realizada exit√≥samente.");			
		} catch (Exception e) {
			log.error("error " + e.getMessage(), e);
			showMessageError(e.getMessage(), e.getMessage());
		}
	}	
