@Controller
public class LandingController {
	private static final Logger log = LoggerFactory.getLogger(LandingController.class);
	
	@RequestMapping("/landing")
	public String landing(@CurrentUser User user) {
		//model.addAttribute("username", 	user.getUsername());
		log.info("Landing ... @CurrentUser " + user.getUsername());
		SecurityUtils.getRolesIfSignedIn();
		if (SecurityUtils.getRolesIfSignedIn().size()>0) {
			log.info("TO redirect:/index.xhtml");
			return "redirect:/index.xhtml";			
		}
		//return "landing";
		log.info("TO logout:/logout");
		return "redirect:/logout";		
	}

}
