	}

	/**
	 * Metodo que cuenta el numero de coincidencias de una cadena en otra
	 * 
	 * 
	 * @param cadena
	 * @param buscar
	 * @return
	 */
	public static Integer countString(String cadena, String buscar) {
		int count = 0;
		int idx = 0;

		while ((idx = cadena.indexOf(buscar, idx)) != -1) {
			idx++;
			count++;
		}
		return count;
	}

